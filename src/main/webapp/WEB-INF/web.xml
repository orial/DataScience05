<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>HomeServlet</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>RegistroServlet</servlet-name>
        <servlet-class>servlets.RegistroServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registro2Servlet</servlet-name>
        <servlet-class>servlets.Registro2Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeServlet</servlet-name>
        <servlet-class>servlets.HomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MovieServlet</servlet-name>
        <servlet-class>servlets.MovieServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RateMovieServlet</servlet-name>
        <servlet-class>servlets.RateMovieServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveRatingServlet</servlet-name>
        <servlet-class>servlets.SaveRatingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MetodoServlet</servlet-name>
        <servlet-class>servlets.MetodoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistroServlet</servlet-name>
        <url-pattern>/RegistroServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registro2Servlet</servlet-name>
        <url-pattern>/Registro2Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeServlet</servlet-name>
        <url-pattern>/HomeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MovieServlet</servlet-name>
        <url-pattern>/MovieServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RateMovieServlet</servlet-name>
        <url-pattern>/RateMovieServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveRatingServlet</servlet-name>
        <url-pattern>/SaveRatingServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MetodoServlet</servlet-name>
        <url-pattern>/MetodoServlet</url-pattern>
    </servlet-mapping>
</web-app>
